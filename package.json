{
  "name": "abp-react-template-basic",
  "version": "0.7.6",
  "description": "ABP React Starter Template - Basic",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@abpjs/account": "file:../abp-react/packages/account",
    "@abpjs/core": "file:../abp-react/packages/core",
    "@abpjs/theme-basic": "file:../abp-react/packages/theme-basic",
    "@abpjs/theme-shared": "file:../abp-react/packages/theme-shared",
    "@chakra-ui/react": "^3.3.1",
    "@emotion/react": "^11.14.0",
    "@reduxjs/toolkit": "^2.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.0.0",
    "react-router-dom": "^7.1.1"
  },
  "pnpm": {
    "overrides": {
      "@abpjs/core": "file:../abp-react/packages/core",
      "@abpjs/theme-shared": "file:../abp-react/packages/theme-shared",
      "@abpjs/theme-basic": "file:../abp-react/packages/theme-basic",
      "@abpjs/account": "file:../abp-react/packages/account"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7"
  }
}
